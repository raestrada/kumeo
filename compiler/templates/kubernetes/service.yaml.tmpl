apiVersion: v1
kind: Service
metadata:
  name: {{agent_id}}-{{workflow_name}}
  labels:
    app: {{agent_id}}
    workflow: {{workflow_name}}
    kumeo.io/agent-type: {{agent_type}}
spec:
  selector:
    app: {{agent_id}}
    workflow: {{workflow_name}}
  ports:
  - port: 80
    targetPort: 8080
    protocol: TCP
    name: http
  type: ClusterIP
